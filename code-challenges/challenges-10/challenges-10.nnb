        {
            "language": "javascript",
            "source": [
                "function compareTriplets(a, b) {\n    let aResult=0;\n    let bResult=0;\n    for(let i=0;  i<3; i++){\n        if( a[i]> b[i]){\n             aResult+=1;\n        }\n        else {\n        if( a[i]< b[i]){\n             bResult+=1;   \n              }}}\n    let n=[aResult,bResult];\n    return n;\n    }"

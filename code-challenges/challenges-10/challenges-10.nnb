            "source": [
                "function migratoryBirds(arr) {\n    // Write your code here\nlet freq = {};\nlet max= 0;\n let maxType = 0;\n    \n    for(let i = 0; i < arr.length; i++){\n        if(freq[arr[i]]){\n            freq[arr[i]] += 1;\n        }else{\n            freq[arr[i]] = 1;\n        }\n    }\n    \n    for(let j in freq){\n        if(freq[j] > max){\n            max = freq[j];\n            maxType = j;\n        }\n    }\n    \n    return maxType;\n}"
